# Database
DATABASE_URL=postgresql://postgres:your-super-secret-password@localhost:54322/postgres
DB_PASSWORD=your-super-secret-password

# Supabase
NEXT_PUBLIC_SUPABASE_URL=http://localhost:54321
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key
JWT_SECRET=your-super-secret-jwt-token-with-at-least-32-characters-long

# Auth
SITE_URL=http://localhost:3000
API_EXTERNAL_URL=http://localhost:54321
{{#if features.authentication}}
ENABLE_EMAIL_SIGNUP=true
DISABLE_SIGNUP=false
MAILER_AUTOCONFIRM=true
SMTP_ADMIN_EMAIL=admin@example.com
SMTP_HOST=mailhog
SMTP_PORT=1025
{{/if}}

{{#if features.aiIntegration}}
# OpenAI
OPENAI_API_KEY=sk-your-openai-api-key
OPENAI_ORG_ID=org-your-org-id

# LangFuse (AI Observability)
LANGFUSE_SECRET=langfuse-secret
LANGFUSE_SALT=langfuse-salt
LANGFUSE_PUBLIC_KEY=pk-lf-your-public-key
LANGFUSE_SECRET_KEY=sk-lf-your-secret-key
LANGFUSE_HOST=http://localhost:3001
{{/if}}

{{#if features.payments}}
# Stripe
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
STRIPE_WEBHOOK_SECRET=whsec_your-webhook-secret
{{/if}}

{{#if features.email}}
# Resend
RESEND_API_KEY=re_your-resend-api-key
{{/if}}

{{#if features.analytics}}
# Analytics
NEXT_PUBLIC_POSTHOG_KEY=phc_your-posthog-key
NEXT_PUBLIC_POSTHOG_HOST=https://app.posthog.com
{{/if}}

# Application
NODE_ENV=development
NEXT_PUBLIC_APP_URL=http://localhost:3000
